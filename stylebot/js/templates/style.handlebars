<div class="style" tabindex=0>
    <span class="style-url-container">
        <input class="toggle-style" type="checkbox" title="Enable or disable this style" tabindex=-1 checked={{enabled}} />
        <div class="style-url" value="{{url}}">
            {{url}}
        </div>
    </span>
    <div class="buttons">
        <div title="Share Style" class="share-style" tabindex=0></div>
        <div title="Edit Style" class="edit-style" tabindex=0></div>
        <div title="Delete Style" class="delete-style close-button" tabindex=0></div>
    </div>
</div>
